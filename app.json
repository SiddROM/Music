{
    "name": "Uta The Diva",
    "description": "Uta is a Powerful Telegram Music Bot. Add it ad a admin in your Group and Listen to Songs anytime.",
    "logo": "https://te.legra.ph/file/d4c8e57cb4b1885f0521d.jpg",
    "keywords": [
        "Telegram",
        "MusicPlayer",
        "VideoPlayer"
    ],
    "repository": "https://github.com/Sherif-Sami/Music",
    "env": {
        "SESSION": {
            "description": "Pyrogram String Session",
            "required": true
        },
        "API_ID": {
            "description": " APP ID fill here my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "API hash fill here my.telegram.org/apps",
            "required": true
        },
        "HNDLR": {
            "description": "put your command handler ,leave it if u use . as a command handler",
            "required": false
        },
        "AlIVE_MSG": {
            "description": "Enter your alive msg",
            "required": false
        },
        "ALIVE_PIC": {
            "description": " Enter your alive pic",
            "required": false
        },
        "PING_MSG": {
            "description": "Enter your ping msg",
            "required": false
        },
        "SUDO_USERS": {
            "description": "fill sudo user for VC plugin ",
            "required": true
        }
    },
    "addons": [],
    "buildpacks": [
        {
            "url": "heroku/python"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "stack": "container"
}
